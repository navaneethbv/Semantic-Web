     xmlns:vc="http://nwalsh.com/rdf/vCard#">
    <owl:Ontology rdf:about="http://utdallas.semtech/class">
        <owl:imports rdf:resource="http://nwalsh.com/rdf/vCard"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://utdallas.semtech/class#maidenName -->

    <owl:DatatypeProperty rdf:about="&sc;maidenName">
        <rdfs:subPropertyOf rdf:resource="&vc;family-name"/>
        <rdfs:domain rdf:resource="&sc;MarriedWoman"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://nwalsh.com/rdf/vCard#Name -->

    <rdf:Description rdf:about="&vc;Name">
        <rdfs:subClassOf rdf:resource="&sc;Person"/>
    </rdf:Description>
    


    <!-- http://utdallas.semtech/class#Man -->

    <owl:Class rdf:about="&sc;Man">
        <rdfs:subClassOf rdf:resource="&sc;Person"/>
        <owl:disjointWith rdf:resource="&sc;Woman"/>
    </owl:Class>
    


    <!-- http://utdallas.semtech/class#MarriedWoman -->

    <owl:Class rdf:about="&sc;MarriedWoman">
        <rdfs:subClassOf rdf:resource="&sc;Woman"/>
        <owl:disjointWith rdf:resource="&sc;UnmarriedWoman"/>
    </owl:Class>
    


    <!-- http://utdallas.semtech/class#Person -->

    <owl:Class rdf:about="&sc;Person">
        <rdfs:subClassOf rdf:resource="&vc;Name"/>
    </owl:Class>
    


    <!-- http://utdallas.semtech/class#UnmarriedWoman -->

    <owl:Class rdf:about="&sc;UnmarriedWoman">
        <rdfs:subClassOf rdf:resource="&sc;Woman"/>
    </owl:Class>
    


    <!-- http://utdallas.semtech/class#Woman -->

    <owl:Class rdf:about="&sc;Woman">
        <rdfs:subClassOf rdf:resource="&sc;Person"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://utdallas.semtech/class#Karen -->

    <owl:Thing rdf:about="&sc;Karen">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <maidenName rdf:datatype="&xsd;string">Stephens</maidenName>
    </owl:Thing>
</rdf:RDF>



